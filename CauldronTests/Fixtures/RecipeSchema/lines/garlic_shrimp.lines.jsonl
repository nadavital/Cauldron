{"line_index":0,"text":"Garlic Shrimp","label":"title"}
{"line_index":1,"text":"Ingredients:","label":"header"}
{"line_index":2,"text":"1 lb shrimp","label":"ingredient"}
{"line_index":3,"text":"3 cloves garlic, minced","label":"ingredient"}
{"line_index":4,"text":"2 tbsp butter","label":"ingredient"}
{"line_index":5,"text":"Instructions:","label":"header"}
{"line_index":6,"text":"Pat shrimp dry","label":"step"}
{"line_index":7,"text":"Saute garlic in butter","label":"step"}
{"line_index":8,"text":"Add shrimp and cook 3 minutes","label":"step"}
{"line_index":9,"text":"Serve with lemon wedges","label":"step"}
